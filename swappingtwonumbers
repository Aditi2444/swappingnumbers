#include <stdio.h>
int main()
{
    int a,b;
    printf("enter value of a");
    scanf("%d",&a);
    printf("enter value of b");
    scanf("%d",&b);
    //swapping
    int temp=a;
    a=b;
    b=temp;
    printf("\nafter swapping a:%d\n", a);
    printf("\nafter swapping b:%d\n", b);
    return 0;
}
